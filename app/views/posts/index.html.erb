<%if user_signed_in? %>
<button><%= link_to "New Post", new_post_path %> </button>
<button><%= link_to 'Logout', destroy_user_session_path, :method => :delete %></button>
<% end %>
<% @posts.each do |post| %>
	<div class="post_wrapper">
    <h2 class="title"><%= link_to post.title, post %></h2>


	</div>

	<% end %>